{
  "name": "delaneymethod/craftyalgolia",
  "description": "A crafty algolia plugin by DelaneyMethod",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "plugin",
    "crafty",
    "algolia",
    "craft-plugin",
    "crafty-algolia"
  ],
  "support": {
    "docs": "https://github.com/delaneymethod/craftyalgolia/blob/master/README.md",
    "issues": "https://github.com/delaneymethod/craftyalgolia/issues"
  },
  "license": "MIT",
  "type": "craft-plugin",
  "prefer-stable": true,
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "DelaneyMethod",
      "email": "hello@delaneymethod.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "craftcms/cms": "^3.1.0"
  },
  "require-dev": {
    "craftcms/cms": "^3.1.0",
    "codeception/codeception": "^3.0",
    "vlucas/phpdotenv": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "delaneymethod\\craftyalgolia\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "name": "CraftyAlgolia",
    "handle": "craftyalgolia",
    "developer": "DelaneyMethod",
    "developerUrl": "https://www.delaneymethod.com",
    "documentationUrl": "https://github.com/delaneymethod/craftyalgolia/blob/master/README.md",
    "changelogUrl": "https://raw.githubusercontent.com/delaneymethod/craftyalgolia/master/CHANGELOG.md",
    "class": "delaneymethod\\craftyalgolia\\CraftyAlgolia"
  }
}
